{
  "name": "@henderea/node-utils",
  "version": "2.6.20",
  "description": "Some node utility scripts",
  "main": "index.js",
  "author": "Eric Henderson <henderea@gmail.com>",
  "repository": "henderea/node-utils.git",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "ncc:build": "yarn ncc:build:release:all",
    "ncc:clean:all": "yarn ncc:clean bin/*.mjs",
    "ncc:clean": "./scripts/ncc-clean.sh",
    "ncc:post-clean": "./scripts/ncc-post_clean.sh",
    "ncc:build:release:build": "./scripts/ncc-build-release-build.sh",
    "ncc:build:debug:build": "./scripts/ncc-build-debug-build.sh",
    "ncc:build:release": "./scripts/ncc-build-release.sh",
    "ncc:build:debug": "./scripts/ncc-build-debug.sh",
    "ncc:build:release:all": "yarn ncc:build:release bin/*.mjs",
    "ncc:build:debug:all": "yarn ncc:build:debug bin/*.mjs",
    "prepublishOnly": "yarn ncc:build",
    "build": "yarn ncc:build",
    "lint": "eslint bin lib --ext .js,.mjs,.ts",
    "lint:fix": "eslint --fix bin lib --ext .js,.mjs,.ts"
  },
  "devDependencies": {
    "@henderea/arg-helper": "^1.2.4",
    "@henderea/my-ncc": "^1.3.9",
    "@henderea/simple-colors": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "any-shell-escape": "^0.1.1",
    "arg": "^5.0.2",
    "chalk": "^5.2.0",
    "cli-columns": "^4.0.0",
    "cli-cursor": "^4.0.0",
    "concat-stream": "^2.0.0",
    "diff": "^5.1.0",
    "eslint": "^8.38.0",
    "eslint-config-henderea": "1.1.7",
    "eslint-plugin-import": "^2.27.5",
    "execa": "^7.1.1",
    "figures": "^5.0.0",
    "iconv-lite": "^0.6.3",
    "inquirer": "^9.1.5",
    "jschardet": "^3.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.43",
    "natural-orderby": "^3.0.2",
    "pushover-notifications": "^1.2.2",
    "rxjs": "^7.8.0",
    "simple-git": "^3.17.0",
    "ttys": "^0.0.3",
    "typescript": "^5.0.4",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32",
    "yargs": "17.7.1"
  },
  "bin": {
    "xpath-get": "dist/xpath-get/index.js",
    "xpath-set": "dist/xpath-set/index.js",
    "moment-date": "dist/moment-date/index.js",
    "nat-exec": "dist/nat-exec/index.js",
    "my-service": "dist/my-service/index.js",
    "strip-accents": "dist/strip-accents/index.js",
    "nsort": "dist/nsort/index.js",
    "fsizewatch": "dist/fsizewatch/index.js",
    "git-add-interactive": "dist/git-add-interactive/index.js",
    "git-select-interactive": "dist/git-select-interactive/index.js",
    "my-math": "dist/my-math/index.js",
    "pushover-notify": "dist/pushover-notify/index.js",
    "json-value": "dist/json-value/index.js"
  },
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "eslintConfig": {
    "extends": "henderea",
    "rules": {
      "no-constant-condition": 0
    }
  }
}
